<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <payment>
            <wibond>
                <active>1</active>
                <model>Wibond\Wibond\Model\Payment\Wibond</model>
                <order_status>pending_payment</order_status>
                <title>Wibond</title>
                <resume_title>Pedido #%1</resume_title>
                <allowspecific>1</allowspecific>
                <specificcountry>AR</specificcountry>
                <group>offline</group>
                <status_pay>processing</status_pay>
                <can_order>false</can_order>
                <can_authorize>false</can_authorize>
            </wibond>
        </payment>
    </default>
</config>
